{
  "name": "mean-ts-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js Express Angular apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cirou/mean-ts-starter"
  },
  "author": "Ciro Aiello",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint && npm run build-ng && npm run copy-assets",
    "serve": "node dist/server.js",
    "build-ng": "ng build",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-assets": "ts-node copyAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "async": "^2.6.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-beta.2",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.0",
    "core-js": "^2.5.3",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "fbgraph": "^1.4.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "lusca": "^1.5.2",
    "mongoose": "^4.13.11",
    "morgan": "^1.9.1",
    "nodemailer": "^4.6.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "rxjs": "^5.4.2",
    "tether": "^1.4.3",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/language-service": "^5.2.11",
    "@types/async": "^2.4.2",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.33",
    "@types/connect-mongo": "0.0.34",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.12",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.6",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.133",
    "@types/lusca": "^1.5.0",
    "@types/mongodb": "^2.2.24",
    "@types/mongoose": "^4.7.45",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.14.6",
    "@types/nodemailer": "^1.3.32",
    "@types/passport": "^0.3.3",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "codelyzer": "~3.1.1",
